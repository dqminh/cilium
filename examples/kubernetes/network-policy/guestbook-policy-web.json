{
  "apiVersion": "extensions/v1beta1",
  "kind": "NetworkPolicy",
  "metadata": {
    "name": "guestbook-web",
    "namespace": "cilium-example"
  },
  "spec": {
    "podSelector": {
      "matchLabels": {
        "k8s-app.guestbook": "web"
      }
    },
    "ingress": [
      {
        "from": [
          {
            "podSelector": {}
          }
        ],
        "ports": [
          {
            "protocol": "TCP",
            "port": 3000
          }
        ]
      }
    ]
  }
}
